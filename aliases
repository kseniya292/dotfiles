# Shortcuts
alias ...='../..'
alias rials="rails"
alias zrc="vim ~/.zshrc"
alias vrc="vim ~/.vimrc"
alias hosts="sudo vim /private/etc/hosts"
alias flow="cat ~/Dropbox/3PH/GithubFlow.md" 
alias applog="grc powder applog"
alias ws="ruby -run -e httpd . -p8000"

# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local
